#include <stdio.h>

int main()
{
    int n,temp,rem,sum=0;
    scanf("%d",&n);
    temp=n;
    while(temp>0){
        rem=temp%10;
        sum=sum+rem;
        temp=temp/10;
    }
    if(sum<10&&sum>=0)
    printf("SUM OF DIGIT IS SINGLE DIDIGT");
    else
    printf("More than Single digit");

    return 0;
}
