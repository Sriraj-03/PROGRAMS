#include<stdio.h>
int main ()
{

    int n,m,i, j,sum,temp;
    scanf("%d %d",&n,&m);
    int matrix[n][m];

    for(i=0;i<n;i++)
    {       
        for(j=0;j<m;j++)
        {

            scanf("%d",&matrix[i][j]);
        }
    }

    for(i=0;i<m;i++)
    {           
        sum=0;          
        for(j=0;j<n;j++)
        {
            sum+=matrix[i][j];                                              
        }
        printf("%d\n",sum);
    }
}